
    <h1>Agende sua Consulta</h1>
    <form action="/cadastrarAgendamento" method="POST">
        <label for="nome">Nome:</label>
        <input type="text" id="nome" name="nome" required><br>

        <label for="nascimento">Data de Nascimento:</label>
        <input type="date" id="nascimento" name="nascimento" required><br>

        <label for="telefone">Telefone:</label>
        <input type="tel" id="telefone" name="telefone" required><br>

        <label for="email">Email:</label>
        <input type="email" id="email" name="email" required><br>

        <label for="datahorario">Data e Hora:</label>
        <input type="datetime-local" id="datahorario" name="datahorario" required><br>

        <label for="espmedica">Especialidade MÃ©dica:</label>
        <input type="text" id="espmedica" name="espmedica" required><br>

        <label for="causa">Causa da Consulta:</label>
        <input type="text" id="causa" name="causa" required><br>

        <button type="submit">Agendar</button>
    </form>

